{
  "name": "glance",
  "version": "1.0.0",
  "description": "A module for inspecting tricky objects.",
  "type": "module",
  "license": "ISC",
  "author": {
    "name": "Chris DiLorenzo",
    "url": "https://dilorenzo.se"
  },
  "repository": "https://github.com/chrisdl/glancejs",
  "type": "module",
  "module": "./esm/index.js",
  "main": "./cjs/index.js",
  "exports": {
    "import": "./esm/index.js",
    "default": "./cjs/index.js"
  },
  "keywords": [
    "glance",
    "glancejs",
    "debugging",
    "debug",
    "simplify objects",
    "debug big objects"
  ],
  "standard": {
    "ignore": [
      "cjs/index.js",
      "esm/index.js",
      "bundle.browser.min.js"
    ]
  },
  "watch": {
    "test": "*.js"
  },
  "scripts": {
    "test": "jest --verbose",
    "watch": "npm-watch",
    "standard": "standard --fix",
    "build": "rollup --config"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.11.0",
    "jest": "^26.4.2",
    "npm-watch": "^0.7.0",
    "rollup": "^2.26.8",
    "rollup-plugin-terser": "^7.0.1",
    "standard": "^14.3.4"
  },
  "dependencies": {}
}
